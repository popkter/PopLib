<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main2Activity">

    <com.pop.viewlib.seekbar.PopSeekBar
        android:layout_width="500dp"
        android:layout_height="200dp"
        android:padding="20dp"
        android:layout_marginTop="100dp"
        app:progressSolidColor="@color/black"
        app:trackSolidColor="@color/design_default_color_surface"
        app:commonRadius="30dp"
        app:naturalProcess="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.pop.viewlib.seekbar.PopSeekBar
        android:layout_width="500dp"
        android:layout_height="200dp"
        android:layout_marginTop="400dp"
        app:progressSolidColor="@color/black"
        app:trackSolidColor="@color/design_default_color_surface"
        app:commonRadius="20dp"
        app:naturalProcess="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>